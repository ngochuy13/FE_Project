mixin introduction-text(title)
  .introduction-text&attributes(attributes)
    .container
      h1.title #{title}
      .editor-content
        if block
          block

mixin introduction-custom(title)
  .introduction-text&attributes(attributes)
    h1.title #{title}
    .editor-content
      if block
        block


