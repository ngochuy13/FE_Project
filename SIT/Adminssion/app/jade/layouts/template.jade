            a(href="#", target="_blank").instagram
              span.fa.fa-instagram
            a(href="#", target="_blank").twitter-square
              span.fa.fa-twitter-square
            a(href="#", target="_blank").youtube-square
              span.fa.fa-youtube-square
          .copy-right.hidden-lg.hidden-md Copyright 2015. Singapore Institute of Technology.    
          

