include ../mixins/all
block vars
  -title = 'Site'
  -bodyClass = ''
- var homepage                = "home.html";
- var physical                = "physical_science_engineering.html";
- var courseDetail            = "course-details.html";
doctype html
//[if IE 8]><html lang="en" class="no-js ie ie8"><![endif]
