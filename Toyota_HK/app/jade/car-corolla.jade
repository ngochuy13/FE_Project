extends layouts/layout

block vars
  -title = 'Car Corolla'
  -bodyClass = 'car-detail' 


block pagescript
  // build:js scripts/home-dep.js
  //- script(src='/scripts/lib-js/__static__.js')
  script(src='/scripts/lib-js/imagesloaded.pkgd.min.js')
  script(src='/scripts/lib-js/masonry.pkgd.min.js')
