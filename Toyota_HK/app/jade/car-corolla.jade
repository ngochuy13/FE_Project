block container

  //- sub-page-banner
  .sub-page-banner
    picture
      source(srcset='images/Cars/Corolla/banner-1-small.jpg', media='(max-width: 1024px)')
      img(src='images/Cars/Corolla/banner-1.jpg', alt='Home banner.', data-width='1920', data-height='799')
    .block-caption
      .block-right-middle
        .title-small Open minded
        h1.title
          | COROLLA
          .price $168,890 up
        .description
          .editor-content
            p
              | Elevate your drive, laoreet dolore magna aliquam erat volutpat
              br
              | consectetuer adipiscing elit

  +car-carousel-gallery([
    {
      'mainImg': 'images/Cars/Corolla/carousel-main-img-01.jpg',
      'thumb'  : 'images/Cars/Corolla/img-3.jpg'
    },
    {
      'mainImg': 'images/Cars/Corolla/carousel-main-img-02.jpg',
      'thumb'  : 'images/Cars/Corolla/img-4.jpg'
