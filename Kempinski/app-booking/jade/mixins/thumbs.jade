              .input-with-icon
                input#b-date-from(type="text" placeholder="FROM DATE" data-mindate="#b-date-to").datetimepicker
                .icon-calendar
              .input-with-icon
                input#b-date-to(type="text" placeholder="TO DATE" data-mintoday="true").datetimepicker
                .icon-calendar
            - else
              .input-with-icon
                input#t-date-from(type="text" placeholder="FROM DATE" data-mindate="#t-date-to").datetimepicker
                .icon-calendar
              .input-with-icon
                input#t-date-to(type="text" placeholder="TO DATE" data-mintoday="true").datetimepicker
                .icon-calendar
            button.btn.btn-default(href='#', role='button') SEARCH

          
          .block-pagination.row
            .col-xs-12.col-sm-4
              .pager
                span Page: 1 of 46

            nav.col-xs-12.col-sm-8.pagination-wrapper
              ul.pagination
                li.prev 
                  a(href='#') prev
                li 
                  a(href='#') 1
                li 
                  a(href='#') 2
                li 
                  a(href='#') 3
                li 
                  a(href='#').current 4
                li 
                  a(href='#') 5
                li 
                  a(href='#') 6
                li 
                  a(href='#') 7
                li 
                  a(href='#') 8
                li 
                  a(href='#') 9
                li.next
