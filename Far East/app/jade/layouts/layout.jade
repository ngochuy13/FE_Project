include ../mixins/all
block vars
  -title = 'Site'
  -bodyClass = ''
- var homepage              = "home.html";
- var residential           = "residential.html";
- var retail                = "retail.html";
- var homepageb             = "homeb.html";
- var search                = "search.html";
doctype html
//[if IE 8]><html lang="en" class="no-js ie ie8"><![endif]
//[if IE 9]><html lang="en" class="no-js ie ie9"><![endif]
//[if gt IE 9]><!
html(class="no-js", lang="en")
  //<![endif]
  head
    meta(charset="utf-8")
    //[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]
    title=title
    meta(name="description", content="")
    meta(name="keywords", content="")
    meta(name="robots", content="index, follow")
    meta(name="author", content="")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    //- link(rel="shortcut icon", href="images/favicon.ico", type="image/x-icon")
    link(rel="stylesheet", href="styles/main.css")
    //- link(href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css", rel="stylesheet")

    //- script include within the block build:js -- endbuild will be combined into one file and the url as stated after build:js
    //- // build:js scripts/modernizr.js
    //- script(src='/bower_components/modernizr/modernizr.js')
    //- // endbuild
  body(class=bodyClass)
    #container
      .page-wrapper
        include ../includes/header
        block container
      include ../includes/footer

    //- cdn links
    //- jQuery https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js
    //- script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js")

    // build:js scripts/cdn.js
    script(src='/bower_components/jquery/dist/jquery.js')
    // endbuild

    // build:js scripts/dep.js
    script(src='/bower_components/pubsub/odfc-pubsub.js')
    script(src='/bower_components/video.js/dist/video-js/video.js')
    script(src='/bower_components/iCheck/icheck.js')
    script(src='/bower_components/bootstrap-sass/assets/javascripts/bootstrap.js')
    script(src='/bower_components/bootstrap-select/dist/js/bootstrap-select.min.js')
    script(src='/bower_components/owl-carousel2/dist/owl.carousel.js')
    script(src='/bower_components/ionrangeslider/js/ion.rangeSlider.js')
    script(src='/bower_components/matchHeight/jquery.matchHeight.js')
    script(src='/bower_components/cookies-js/dist/cookies.js')
    // endbuild
  
    //- // build:js scripts/components.js
    //- script(src='/scripts/navigation.js')
    //- // endbuild

    block pagescript

    // build:js scripts/main.js
    script(src='/scripts/main.js')
    // endbuild

    .hide &nbsp;



