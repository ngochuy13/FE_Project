			.container
				ul.nav.nav-tabs.locator-tabs(role="tablist")
					li.tac.active(role="presentation")
						a.bold(href="#suplier" aria-controls="suplier" role="tab" data-toggle="tab") Find a Supplier
					li.tac(role="presentation")
						a.bold(href="#builder" aria-controls="profile" role="tab" data-toggle="tab") Find a Builder
					li.tac(role="presentation")
						a.bold(href="#installer" aria-controls="messages" role="tab" data-toggle="tab") Find an Installer
				.tab-content.locator-tab-content
