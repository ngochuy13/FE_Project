			.container
				.row
					.header-item
						.col-sm-1.hidden-xs
							.image.inner
								img(src="images/favourite/favourite.png")
						.col-sm-2
							.content.inner
								h3.bold My Favourites
								.print.hidden-sm.hidden-xs
